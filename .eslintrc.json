{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jasmine": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "no-plusplus": 0,
        "no-console": 0,
        "import/no-useless-path-segments": 0,
        "no-underscore-dangle": 0,
        "import/no-extraneous-dependencies": 0
    },
    "plugins": ["jasmine"]
}
